#parks {
  polygon-opacity:1;
  polygon-fill:#70a770;
  polygon-pattern-file:url("/Users/incanus/Desktop/map data/grass.jpg");
  polygon-pattern-opacity:0.25;
  line-width:1;
  line-color:green;
}

#streets {
  line-width:2;
  line-color:#666;
  line-opacity:0.5;
}

#buildings {
  polygon-opacity:1;
  polygon-fill:#f9f0cc;
  polygon-pattern-file:url("/Users/incanus/Desktop/map data/stripes.png");
  polygon-pattern-opacity:0.5;
  line-color:gray;
  line-width:1;
}

#rail {
  line-width:2;
  line-color:#747070;
}

#stops {
  marker-width:8;
  marker-fill:#747070;
  marker-allow-overlap:true;
}

#water {
  polygon-opacity:1;
  polygon-fill:#b8e0f1;
  polygon-pattern-file:url("/Users/incanus/Desktop/map data/waves.jpg");
  polygon-pattern-opacity:0.25;
  line-width:1;
  line-color:#00f;
}

#land {
  polygon-fill:#fabc92;
  polygon-opacity:0.25;
  polygon-pattern-file:url("/Users/incanus/Desktop/map data/masonry.jpg");
  polygon-pattern-opacity:0.1;
}

#banks {
  text-name:"'$'";
  text-fill:green;
  text-halo-fill:white;
  text-halo-radius:2;
  text-face-name: "Verdana Bold";
  text-size:15;
}

#carts {
  marker-width:15;
  marker-fill:#eb4e22;
  marker-allow-overlap:true;
  marker-fill-opacity:0.5;
}